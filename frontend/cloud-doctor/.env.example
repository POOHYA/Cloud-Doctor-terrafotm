# HTTPS를 활성화할지 여부 (true/false)
HTTPS=true

# 로컬 개발용 SSL 인증서 경로 (HTTPS=true일 때 사용)
SSL_CRT_FILE=./localhost.pem

# 로컬 개발용 SSL 키 파일 경로 (HTTPS=true일 때 사용)
SSL_KEY_FILE=./localhost-key.pem

# 개발 서버가 동작할 호스트명
HOST=localhost

# 개발 서버가 동작할 포트 번호
PORT=3000

# 감사(Audit) API 서버 주소 (React에서 사용)
REACT_APP_AUDIT_API_URL=https://localhost:8000

# 메인 백엔드 API 서버 주소 (React에서 사용)
REACT_APP_API_URL=https://localhost:9000
